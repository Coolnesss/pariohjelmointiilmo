<p id="notice"><%= notice %></p>

<h1>Listing Opiskelijas</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Aika</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @opiskelijas.each do |opiskelija| %>
      <tr>
        <td><%= opiskelija.numero %></td>
        <td></td>
        <td><%= opiskelija.aika.aika %></td>
      </tr>
    <% end %>
  </tbody>
</table>
